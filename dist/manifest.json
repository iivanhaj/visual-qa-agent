{
  "manifest_version": 3,
  "name": "Visual QA Agent - Advanced Bug Detector",
  "version": "2.0.0",
  "description": "Comprehensive web bug detection with SEO, accessibility, performance, and security testing. Export actionable design docs for engineers and AI tools.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs",
    "downloads",
    "sidePanel"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "Visual QA Agent"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.ts-loader-DxReOCsI.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+Shift+T"
      },
      "description": "Open Visual QA Agent Sidebar"
    }
  },
  "icons": {
    "16": "public/vite.svg",
    "48": "public/vite.svg",
    "128": "public/vite.svg"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/aiSuggestionService-D9QXck87.js",
        "assets/index-DU3xAm2I.js",
        "assets/content.ts-qrpfv_mA.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
